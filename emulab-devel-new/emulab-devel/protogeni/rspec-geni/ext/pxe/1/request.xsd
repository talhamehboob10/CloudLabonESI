<?xml version="1.0" encoding="UTF-8"?>
<!-- An extension for specifying PXE boot images for nodes -->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" targetNamespace="http://www.protogeni.net/resources/rspec/ext/pxe/1" xmlns:pxe="http://www.protogeni.net/resources/rspec/ext/pxe/1">
  <!-- This is meant to extend the services element -->
  <xs:element name="services_pxe_boot">
    <xs:complexType>
      <xs:attribute name="name" use="required" type="xs:string"/>
    </xs:complexType>
  </xs:element>
  <!-- Both of the above are start elements. -->
</xs:schema>
