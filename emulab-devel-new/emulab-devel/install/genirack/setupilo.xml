<RIBCL VERSION="2.0">
 <LOGIN USER_LOGIN="Administrator" PASSWORD="%s">
  <USER_INFO MODE="write">
    <ADD_USER 
      USER_NAME="elabman" 
      USER_LOGIN="elabman" 
      PASSWORD="%s">
      <ADMIN_PRIV value ="Y"/>
      <REMOTE_CONS_PRIV value ="Y"/>
      <RESET_SERVER_PRIV value ="Y"/>
      <VIRTUAL_MEDIA_PRIV value ="Y"/>
      <CONFIG_ILO_PRIV value="Yes"/>
    </ADD_USER>
  </USER_INFO>
  <SERVER_INFO MODE="write">
    <!-- Enable automatic power on with a minimum delay.           -->
    <!-- Note: iLO firmware supports values "Yes" and "No" only.   -->
    <SERVER_AUTO_PWR VALUE="Yes"/>
  </SERVER_INFO>
  <RIB_INFO MODE="write">
    <!-- Set the autologout timeout to 120 minutes                 -->
    <MOD_GLOBAL_SETTINGS>
      <SESSION_TIMEOUT value="120"/>
      <IPMI_DCMI_OVER_LAN_ENABLED value="N"/>
      <SNMP_ACCESS_ENABLED value="N"/>
    </MOD_GLOBAL_SETTINGS>
  </RIB_INFO>
 </LOGIN>
</RIBCL>
