<RIBCL VERSION="2.0">
 <LOGIN USER_LOGIN="Administrator" PASSWORD="%s">
  <USER_INFO MODE="write">
    <MOD_USER USER_LOGIN="Administrator">
      <PASSWORD value="%s"/>
    </MOD_USER>
  </USER_INFO>
  </LOGIN>
</RIBCL>
